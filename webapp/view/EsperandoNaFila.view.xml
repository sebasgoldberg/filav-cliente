<mvc:View
	controllerName="iamsoft.filav.cliente.controller.EsperandoNaFila"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m">

            <Input value="Esperando Na Fila"></Input>

	<List
		items="{/}"
		headerText="Turnos">
		<items>
			<ObjectListItem
				title="{id}"
				type="Active"
				number="3º"
				numberUnit="posição">
				<firstStatus>
					<ObjectStatus
						text="{estado}"
						state="success" />
				</firstStatus>
				<attributes>
					<ObjectAttribute text="HH:MM" />
				</attributes>
			</ObjectListItem>
		</items>
	</List>

</mvc:View>
